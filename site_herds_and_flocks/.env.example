# Frontend Environment Variables
# Copy this file to .env.local for development
# For production, set these in Vercel Dashboard > Settings > Environment Variables

# API URL
# Development: Uses proxy to localhost
# Production: Set to your Railway API URL
REACT_APP_API_URL=/api/v1

# Stripe Publishable Key (safe to expose - this is the PUBLIC key)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx

# Optional: Feature flags
REACT_APP_ENABLE_DEBUG=false
